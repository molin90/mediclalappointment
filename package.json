{
  "name": "webpack-4-react-boilerplate",
  "version": "1.0.0",
  "description": "A simple and complete Webpack 4 React Boilerplate",
  "main": "index.js",
  "repository": "https://github.com/user/repository.git",
  "author": "Your Name <your@email.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config=config/webpack.dev.js --hot --progress",
    "build": "cross-env NODE_ENV=production webpack --config=config/webpack.prod.js --progress && node config/fixSw.js",
    "buildDev": "cross-env NODE_ENV=development webpack --config=config/webpack.dev.js --progress",
    "start:reload": "webpack-dev-server --config=config/webpack.dev.js",
    "pageTools": "node config/scripts/pageTools/pageTools",
    "start:style": "cross-env MAP_STYLE=true webpack-dev-server --config=config/webpack.dev.js --hot",
    "analyze": "cross-env ENABLE_ANALYZER=true yarn build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
